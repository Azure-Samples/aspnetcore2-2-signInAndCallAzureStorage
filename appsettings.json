{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "<tenant>.onmicrosoft.com",
    "TenantId": "<tenantid>",
    "ClientId": "<clientid>",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath ": "/signout-callback-oidc",

    // To call an API
    "ClientSecret": "<client_secret>"

  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
